                        "endpoint": "/search"
                    },
                    {
                        "channel": "select",
                        "endpoint": "/select"
                    },
                    {
                        "channel": "create",
                        "endpoint": "/create"
                    },
                    {
                        "channel": "status",
                        "endpoint": "/status"
                    },
                    {
                        "channel": "confirm",
                        "endpoint": "/confirm"
                    },
                    {
                        "channel": "content-read",
                        "endpoint": "/content/read"
                    }
                ]
            },
            {
                "path": "",
                "values": [
                    {
                        "status": "success",
